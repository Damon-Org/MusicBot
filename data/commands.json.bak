            {
                "name": "play",
                "aliases" : [
                    "p"
                ],
                "description": "Adds the song you request to queue and plays it.",
                "usage": "play <search>",
                "params": [
                    {
                        "name": "search",
                        "description": "Search on YouTube or use a YouTube link.",
                        "type": "string",
                        "required": true,
                        "allow_sentence": true
                    }
                ],
                "examples": [
                    "p https://www.youtube.com/watch?v=rVHn3GOXvzk",
                    "play My House Flo Rida"
                ]
            },
            {
                "disabled": true,
                "name": "playlist",
                "aliases": [],
                "description": "Starts playing a playlist from its share ID, this will append all the songs or if no music is playing a queue will be created and the song will be played instead.",
                "usage": "playlist <share-ID>",
                "params": [
                    {
                        "name": "share-ID",
                        "description": "A Damon Music playlist share ID",
                        "type": "string",
                        "required": true
                    }
                ],
                "examples": [
                    "playlist CXJ3k"
                ]
            },
            {
                "disabled": true,
                "name": "playlists",
                "aliases": [],
                "description": "Shows all your saved playlists.",
