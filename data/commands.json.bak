                "usage": "resume",
                "params": [],
                "examples": []
            },
            {
                "name": "skip",
                "aliases": [
                    "next"
                ],
                "description": "Will skip the currently playing song.",
                "usage": "skip",
                "params": [],
                "examples": []
            },
            {
                "name": "skipto",
                "aliases": [],
                "description": "Skip to the given position in queue.",
                "usage": "skipto <#queue-number>",
                "params": [
                    {
                        "name": "queue-number",
                        "description": "Number of a song in queue",
                        "type": "number",
                        "required": true
                    }
                ],
                "examples": []
            },
            {
                "disabled": true,
                "name": "saveplaylist",
                "aliases": [
                    "savepl"
                ],
                "description": "Will save the current queue as playlist.",
                "usage": "saveplaylist",
                "params": [],
                "examples": []
