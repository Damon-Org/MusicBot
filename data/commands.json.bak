    "moderation": {
        "guild_only": true,
        "commands": [
            {
                "name": "lock",
                "aliases": [],
                "description": "Lock specific feature to one channel",
                "usage": "lock <feature> [#channel]",
                "params": [
                    {
                        "name": "feature",
                        "description": "Lock a command category to one channel",
                        "type": "string",
                        "required": true
                    },
                    {
                        "name": "#channel",
                        "description": "Mention a channel with #channelname",
                        "type": "reference",
                        "default": "Takes the channel the command was ran in"
                    }
                ],
                "permission": {
                    "type": "server",
                    "name": "MANAGE_CHANNELS"
                },
                "examples": [
                    "lock music #music"
                ]
            },
            {
                "name": "removelock",
                "aliases": [
                    "remlock",
                    "rmlock"
                ],
                "description": "Remove the feature lock if one was in place.",
                "usage": "removelock <feature>",
                "params": [
                    {
                        "name": "feature",
                        "description": "Lock a command category to one channel",
                        "type": "string",
                        "required": true
                    }
                ],
                "permission": {
                    "type": "server",
                    "name": "MANAGE_CHANNELS"
                },
                "examples": [
                    "rmlock music",
                    "remlock music"
                ]
            },
            {
                "name": "resetprefix",
                "aliases": [
                    "rprefix"
                ],
                "description": "Reset the prefix to the default value.",
                "usage": "resetprefix",
                "params": [],
                "permission": {
                    "type": "server",
                    "name": "MANAGE_CHANNELS"
                },
                "examples": [
                    "resetprefix",
                    "rprefix"
                ]
            },
            {
                "name": "setprefix",
                "aliases": [
                    "changeprefix"
                ],
                "description": "Change the bot its prefix.",
                "usage": "setprefix <new-prefix>",
                "params": [
                    {
                        "name": "new-prefix",
                        "description": "Changes the prefix to which Damon Music listens on in your server.",
                        "type": "string",
                        "required": true
                    }
                ],
                "permission": {
                    "type": "server",
                    "name": "MANAGE_CHANNELS"
                },
                "examples": [
                    "setprefix !",
                    "changeprefix b?"
                ]
            }
        ]
    },
    "music": {
        "guild_only": true,
        "commands": [
            {
                "name": "leave",
                "aliases": [
                    "quit",
                    "exit",
                    "stop",
                    "brexit",
                    "no-u"
                ],
                "description": "Bot will stop music playback and leave channel",
                "usage": "leave",
                "params": [],
                "examples": []
